/* pages/levelOne/success/success.wxss */
.level-one-success{
    position: relative;
    width: 100vw;
    height: 100vh;
}
.level-one-success .success-bg{
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
}
.level-one-success .subutton{
    position: absolute;
    left: 13%;
    width: 74%;
    height: 5%;
    border-radius: 18rpx;
    font-size: 36rpx;
    color: #FFFFFF;
    display: flex;
    align-items: center;
    justify-content: center;
}
.level-one-success .subutton.continue{
    bottom: 28.8%;
    background: linear-gradient(180deg, #2BDFFC 0%, #13BBF7 100%);
}
.level-one-success .subutton.down{
    bottom: 21%;
    background: linear-gradient(180deg, #BD94F3 0%, #885BE4 100%);
}
.level-one-success .subutton.close{
    bottom: 13.2%;
    background: linear-gradient(180deg, #FF9290 0%, #FE5957 100%);
}